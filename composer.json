	"description": "Di is a dependency injection container",
	"type": "library",
	"version": "1.0",
	"license": "BSD-2-Clause",
	"authors": [
		{
			"name": "Benjamin Carl",
			"email": "opensource@clickalicious.de"
		},
		{
